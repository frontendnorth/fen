{
  "name": "fen",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sass:dev": "node-sass src/assets/scss/all.scss -o docs/assets/css/",
    "sass:watch": "node-sass src/assets/scss/all.scss -wo docs/assets/css/",
    "fonts": "cp -R src/assets/fonts/ docs/assets/fonts/",
    "autoprefix": "npx postcss docs/assets/css/*.css --use autoprefixer -d docs/assets/css/",
    "11ty:build": "npx eleventy",
    "11ty:watch": "npx eleventy --serve",
    "svgmin": "svgo --disable=inlineStyles -r -f src/assets/img -o docs/assets/img",
    "init": "npm run sass:dev && npm run fonts && npm run svgmin && npm run 11ty:build",
    "dev": "npm run init && npm run sass:watch & npm run 11ty:watch",
    "build": "npm run clean && npm run init && npm run autoprefix",
    "clean": "rimraf ./docs",
    "deploy-staging": "npm run build && firebase deploy -P default"
  },
  "keywords": [],
  "author": "Front End North",
  "license": "none",
  "dependencies": {
    "@11ty/eleventy": "^0.8.3",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "molecules": "0.0.3",
    "autoprefixer": "^9.6.0",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2",
    "normalize.css": "^8.0.1",
    "sass-mq": "^5.0.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.3"
  },
  "browserslist": [
    "last 2 versions",
    "ie 11"
  ]
}
