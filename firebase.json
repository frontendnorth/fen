{
  "hosting": {
    "public": "docs",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [{
      "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=31557600"
      }]
    }, {
      "source": "**/*.@(svg|png)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=31557600"
      }]
    }],
    "redirects": [{
      "source": "/blog/2019/09/rebooting-front-end-north/",
      "destination": "/blog/2019/10/rebooting-front-end-north/",
      "type": 301
    }]
  }
}
